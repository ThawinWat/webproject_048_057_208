                <li class="nav-item">
                    <a class="nav-link" href="/home">
                        <i class="bi bi-house"></i> Dashboard
                    </a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="/group_select">
                        <i class="bi bi-bar-chart"></i> Group
